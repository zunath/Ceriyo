<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:data="clr-namespace:Ceriyo.Infrastructure.Network.TransferObjects;assembly=Ceriyo.Infrastructure">

    <!-- Entries must exist here for any objects which need to be data-bound. -->
    <!-- Data Objects -->
    <DataTemplate DataType="{x:Type data:PCTransferObject}">
        <TextBlock Margin="5" Text="{Binding Name}" />
    </DataTemplate>

</ResourceDictionary>